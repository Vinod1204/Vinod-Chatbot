# Copy this file to .env and fill in your API key. Do NOT commit the real .env to git.
OPENAI_API_KEY=sk-your-api-key-here
# Optional: override the default model
# OPENAI_MODEL=gpt-4o-mini

LANGFUSE_PUBLIC_KEY=LANGFUSE_PUBLIC_KEY_HERE
LANGFUSE_SECRET_KEY=LANGFUSE_SECRET_KEY_HERE
LANGFUSE_BASE_URL=https://cloud.langfuse.com

# MongoDB connection (required for user accounts)
# MONGODB_URI=mongodb+srv://username:password@cluster-hostname/?retryWrites=true&w=majority
# MONGODB_DB_NAME=your-database-name

# Session key for browser-based auth flows
# SESSION_SECRET_KEY=change-me-to-a-long-random-string

# Google OAuth (required for "Continue with Google")
# GOOGLE_CLIENT_ID=your-google-oauth-client-id.apps.googleusercontent.com
# GOOGLE_CLIENT_SECRET=your-google-oauth-client-secret
# GOOGLE_REDIRECT_URL=http://localhost:8000/api/auth/oauth/google/callback

# Allowed origins for the frontend
# ALLOWED_ORIGINS=http://localhost:5173,http://127.0.0.1:5173
